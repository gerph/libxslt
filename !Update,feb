dir <Obey$Dir>
cdir RISCOS
cdir RISCOS.o32
cdir RISCOS.aif
cdir RISCOS.breakpoint
cdir RISCOS.breakpoint.c
cdir RISCOS.breakpoint.h
cdir RISCOS.breakpoint.o32
cdir RISCOS.libxslt
cdir RISCOS.libxslt.c
cdir RISCOS.libxslt.h
cdir RISCOS.libxslt.o32
cdir RISCOS.libexslt
cdir RISCOS.libexslt.c
cdir RISCOS.libexslt.h
cdir RISCOS.libexslt.o32
cdir RISCOS.xsltproc
cdir RISCOS.xsltproc.c
cdir RISCOS.xsltproc.h
cdir RISCOS.xsltproc.o32
cdir diffs
cdir diffs.libxslt
cdir diffs.libxslt.c
cdir diffs.libxslt.h
cdir diffs.libexslt
cdir diffs.libexslt.c
cdir diffs.libexslt.h
cdir diffs.xsltproc
cdir diffs.xsltproc.c
cdir diffs.xsltproc.h

Obey <Obey$Dir>.!~Setup

IfThere <ThisRISCOS>.Makefile Then Error Diff and Patch already applied

alias apply Obey !~UpdateH %%*0

| C code
apply libxslt.c.attributes
apply libxslt.c.documents
apply libxslt.c.extensions
apply libxslt.c.extra
apply libxslt.c.functions
apply libxslt.c.imports
