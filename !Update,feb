apply libxslt.c.transform
apply libxslt.c.variables
apply libxslt.c.xslt
apply libxslt.c.xsltutils
apply libexslt.c.common
apply libexslt.c.date
apply libexslt.c.dynamic
apply libexslt.c.exslt
apply libexslt.c.functions
apply libexslt.c.math
apply libexslt.c.sets
apply libexslt.c.saxon
apply libexslt.c.strings
apply xsltproc.c.xsltproc
apply xsltproc.c.throwback

| Headers
apply libxslt.h.config
apply libxslt.h.attributes
apply libxslt.h.documents
apply libxslt.h.extensions
apply libxslt.h.extra
apply libxslt.h.functions
apply libxslt.h.imports
